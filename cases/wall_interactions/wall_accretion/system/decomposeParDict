FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 7;

method          hierarchical;

hierarchicalCoeffs
{
    n               (7 1 1);
	delta           0.001;
    order           xyz;
}


